
#include <iostream>
using namespace std;
int main()
{
    int num;
    int count=0;
    cout<<"enter the num=";
    cin>>num;
    for(int i=1;i<num;i++)
   {
       for(int j=i;j<num;j++)
       {
           for(int k=j;k<num;k++)
           {
               for(int l=k;l<num;l++)
               {
                   if(((i+j+k+l)==num)){
                      count++;
                   cout<<i<<"+"<<j<<"+"<<k<<"+"<<l<<endl;
                
               
                       
                   }
               }
           }
       }
   }    cout<<count;

    return 0;
}
